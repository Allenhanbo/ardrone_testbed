<launch>
  <node pkg="rviz" type="rviz" name="rviz" args="../../master_config.rviz"/>
  <node pkg="rosbag" type="play" name="player" output="screen" args="--clock ../../../woods3.bag"/>
  <node pkg="ardrone_orb" name="ORB_mono" type="ros_orb_node" output="screen" args="$(find ardrone_orb)/ORBvoc.txt  $(find ardrone_orb)/settings.yaml">
    <remap from="/camera/image_raw" to="/ardrone/front/image_raw" />
  </node>
</launch>
